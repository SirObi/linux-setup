<keymap version="1" name="Emacs copy" parent="Emacs">
  <action id="$Undo">
    <keyboard-shortcut first-keystroke="ctrl z" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="u" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl comma" />
  </action>
  <action id="CloseActiveTab">
    <keyboard-shortcut first-keystroke="shift ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="0" />
  </action>
  <action id="CollapseSelection" />
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="alt semicolon" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="semicolon" />
  </action>
  <action id="Editor Undo">
    <keyboard-shortcut first-keystroke="ctrl z" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="u" />
  </action>
  <action id="EditorChooseLookupItemDot" />
  <action id="EditorCutLineEnd" />
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl k" />
  </action>
  <action id="EditorLeft">
    <keyboard-shortcut first-keystroke="left" />
  </action>
  <action id="EditorPageUp">
    <keyboard-shortcut first-keystroke="page_up" />
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="EditorUp">
    <keyboard-shortcut first-keystroke="up" />
  </action>
  <action id="FileChooser.TogglePathShowing" />
  <action id="Flutter.DeviceSelector">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="d" />
  </action>
  <action id="FocusEditor" />
  <action id="GotoClass">
    <keyboard-shortcut first-keystroke="shift alt g" />
    <keyboard-shortcut first-keystroke="ctrl b" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl alt g" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="period" />
    <keyboard-shortcut first-keystroke="alt period" />
    <mouse-shortcut keystroke="control button1" />
    <keyboard-shortcut first-keystroke="ctrl period" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="r" />
  </action>
  <action id="Table-scrollUpChangeSelection">
    <keyboard-shortcut first-keystroke="page_up" />
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="Terminal.SwitchFocusToEditor" />
  <action id="Tree-scrollUpChangeSelection">
    <keyboard-shortcut first-keystroke="page_up" />
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="UpdateRunningApplication" />
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl f10" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="ctrl alt f10" />
  </action>
</keymap>